package br.org.serratec.teste;

import br.org.serratec.model.Empresa;
import br.org.serratec.model.Pessoa;

public class TesteImpostodeRenda {

	public static void main(String[] args) {
		Pessoa pessoa = new Pessoa("Leandro", 6850.);
		Empresa empresa = new Empresa("Alterdata", 100000.);
		
		System.out.println(pessoa);
		System.out.println(empresa + " Tributos: " + empresa.calcularICMS());

	}

}

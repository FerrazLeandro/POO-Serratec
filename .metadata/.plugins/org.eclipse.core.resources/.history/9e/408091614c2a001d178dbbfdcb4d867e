package br.org.serratec.model;

public class AtletaAmador extends Maratona {
	private Boolean atestato;

	public AtletaAmador(String nome, String sexo, Integer idade, Double altura, Boolean atestato) {
		super(nome, sexo, idade, altura);
		this.atestato = atestato;
	}

	public Boolean getAtestato() {
		return atestato;
	}
	
}

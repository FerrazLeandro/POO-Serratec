package org.br.serratec.model;

public class Lutador {
	private String nome;
	private Double peso;
	private Integer idade;
	private Categoria categoria;
	public Lutador(String nome, Double peso, Integer idade, Categoria categoria) {
		super();
		this.nome = nome;
		this.peso = peso;
		this.idade = idade;
		this.categoria = categoria;
	}
	
	
}

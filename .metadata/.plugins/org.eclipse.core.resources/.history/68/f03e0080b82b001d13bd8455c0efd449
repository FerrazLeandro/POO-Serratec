package aula02;

public class CalcularMedia {

	public static void main(String[] args) {
		double nota1 = 7.5, nota2 = 9.0, media = 0.00;
		
		System.out.println("Nota 1: " + nota1);
		System.out.println("Nota 2: " + nota2);
		
		media = (nota1 + nota2) / 2;
		
		System.out.println("MÃ©dia: " + media);
			

	}

}

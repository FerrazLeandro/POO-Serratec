package br.org.serratec.teste;

import br.org.serratec.model.Assistente;
import br.org.serratec.model.Gerente;

public class TesteJuncionario {

	public static void main(String[] args) {
		Gerente gerente = new Gerente("05838594705", "Leandro", 5000., "Financeiro");
		System.out.println("Dados: " + gerente.getNome() + "\n CPF: " + gerente.getCpf());
		
		Assistente assistente = new Assistente("91500265772", "Sidineia", 10000., "Auxiliar");
		System.out.println("Dados: " + assistente.getNome() + "\n CPF: " + assistente.getCpf());

	}

}

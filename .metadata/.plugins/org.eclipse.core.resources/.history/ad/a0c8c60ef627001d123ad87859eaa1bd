package br.org.serratec.exemplos;

public class Exemplo3 {

	public static void main(String[] args) {
		// CTRL + ALT + Seta para cima duplica a linha para cima, seta para baixo duplica para baixo
		// CTRL + D - Apaga a linha
		// CTRL + Delete apaga a instrução
		// ALT + Seta para cima ou para baixo movimenta as linhas
		
		
		System.out.println("POO");
		System.out.println("Programação em Java - Aula 01");
		System.out.println("Programação em Java - Aula 01");
		System.out.println("Programação em Java - Aula 01");
		System.out.println("Programação em Java - Aula 01");
		                                                
	}

}

package br.org.serratec.teste;

import br.org.serratec.model.Pessoa;

public class TestePessoa {

	public static void main(String[] args) {
		Pessoa pessoa = new Pessoa();
		pessoa.setCodigo(123);
		pessoa.setNome("Leandro");
		pessoa.setAltura(1.65);
		pessoa.setPeso(55.0);
		
		System.out.println("Seu IMC é: " + pessoa.resultadoIMC());
		
		
		
		Pessoa pessoa2 = new Pessoa(123, "Leandro", 1.65, 55.0);
		
		System.out.println("Seu IMC é: " + pessoa2.resultadoIMC());
		

	}

}
